// configure vscode when attaching to a docker container

{
  "name": "Django Dev",
  "dockerComposeFile": "../docker-compose-dev.yml",
  "service": "django",
  "workspaceFolder": "/app",
  "remoteUser": "appuser",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python"
      ]
    }
  }
}